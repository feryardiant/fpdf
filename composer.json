{
    "name": "feryardiant/fpdf",
    "type": "library",
    "description": "Unofficial FPDF library with PSR-0 compliant, clean and readable code.",
    "license": "no usage restriction",
    "homepage": "http://www.fpdf.org",
    "authors": [
        {
            "name": "Fery Wardiyanto",
            "email": "ferywardiyanto@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/feryardiant/fpdf/issues"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/feryardiant/fpdf"
        }
    ],
    "suggest": {
        "ext-gd": "For Image Parser support",
        "ext-zlib": "For Compression support"
    },
    "autoload": {
        "psr-0": { "Fpdf": "src" }
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "bin": [ "bin/makefont" ],
    "archive": {
        "exclude": [ "/docs", "/samples", "/tests" ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
